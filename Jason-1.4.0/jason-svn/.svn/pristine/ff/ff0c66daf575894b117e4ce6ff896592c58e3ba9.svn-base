package net.sourceforge.jasonide.core;

/**
 * Exception for Jason Plugin operations.
 * @author Germano
 */
public class JasonPluginException extends Throwable {

    private static final long serialVersionUID = 1L;

    /**
     * Creates a new JasonPluginException. 
     * @param msg
     */
    public JasonPluginException(String msg) {
        super(msg);
    }   
}
